          SQLTask:SqlStatementSource="DECLARE @start_date date&#xA;DECLARE @end_date date&#xA;&#xA;SET @end_date = CURRENT_TIMESTAMP&#xA;SET @start_date = DATEADD(wk, -9, @end_date)  &#xA;SET @end_date = DATEADD(wk, 2, @end_date)&#xA;&#xA;--select @start_date, @end_date&#xA;&#xA;&#xA;DELETE soundmds.[dbo].[ScheduleMasterCurrent]&#xA;WHERE   &#xA;    [prov_sched_info_date_of_shift] BETWEEN @start_date AND @end_date&#xA;&#xA;&#xA;TRUNCATE TABLE etl.MDS.Staging_schedulemaster&#xA;&#xA;SELECT&#xA;    shift_source_site_id = CAST(AcctgSiteId AS varchar(50)), &#xA;    [start_date] = CAST(@start_date AS varchar(50)),&#xA;    end_date = CAST(@end_date AS varchar(50))&#xA;FROM&#xA;    ETL.dbo.Tangier_Control&#xA;WHERE&#xA;    AcctgSiteId IS NOT NULL &#xA;    AND (SiteGoLive &lt;= @start_date)&#xA;ORDER BY&#xA;    shift_source_site_id"
